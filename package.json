  "dependencies": {
    "@semantic-release/changelog": "^6.0.2",
    "@tanstack/react-query": "^4.24.10",
    "@types/multer": "^1.4.7",
    "antd": "^5.1.4",
    "axios": "^1.3.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "file-loader": "^6.2.0",
