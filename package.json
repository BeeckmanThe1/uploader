    "react-dom": "^18.2.0",
    "zustand": "^4.3.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.14",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@webpack-cli/generators": "^3.0.1",
    "css-loader": "^6.7.2",
    "eslint": "^8.29.0",
